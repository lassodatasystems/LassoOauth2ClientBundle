{
    "name": "lasso/oauth2-client-bundle",
    "description": "Access APIs protected by the oauth2 authentication scheme",
    "type": "symfony-bundle",
    "keywords": [
        "oauth", "client", "api", "Symfony2"
    ],
    "homepage": "https://github.com/lassodatasystems",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Thomas Rubbert",
            "email": "thomas@lassocrm.com",
            "role": "developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "juliendidier/buzz-bundle": "2.0.x-dev"
    },
    "autoload": {
        "psr-0": { "Lasso\\Oauth2ClientBundle": "" }
    },
    "target-dir": "Lasso/Oauth2ClientBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
